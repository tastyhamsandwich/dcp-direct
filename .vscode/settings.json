        "activityBarBadge.background": "#000000",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "editorGroup.border": "#442e9b",
        "panel.border": "#442e9b",
        "sash.hoverBorder": "#442e9b",
        "sideBar.border": "#442e9b",
        "statusBar.background": "#332274",
        "statusBar.border": "#332274",
        "statusBar.debuggingBackground": "#637422",
        "statusBar.debuggingBorder": "#637422",
        "statusBar.debuggingForeground": "#e7e7e7",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#442e9b",
        "statusBarItem.remoteBackground": "#332274",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "tab.activeBorder": "#442e9b",
        "titleBar.activeBackground": "#332274",
        "titleBar.activeForeground": "#e7e7e7",
