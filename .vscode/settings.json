{
   "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#2c59ad",
      "activityBar.background": "#2c59ad",
      "activityBar.foreground": "#e7e7e7",
      "activityBar.inactiveForeground": "#e7e7e799",
      "activityBarBadge.background": "#4c1327",
      "activityBarBadge.foreground": "#e7e7e7",
      "commandCenter.border": "#e7e7e799",
      "editorGroup.border": "#2c59ad",
      "panel.border": "#2c59ad",
      "sash.hoverBorder": "#2c59ad",
      "sideBar.border": "#2c59ad",
      "statusBar.background": "#224484",
      "statusBar.border": "#224484",
      "statusBar.debuggingBackground": "#846222",
      "statusBar.debuggingBorder": "#846222",
      "statusBar.debuggingForeground": "#e7e7e7",
      "statusBar.foreground": "#e7e7e7",
      "statusBarItem.hoverBackground": "#2c59ad",
      "statusBarItem.remoteBackground": "#224484",
      "statusBarItem.remoteForeground": "#e7e7e7",
      "tab.activeBorder": "#2c59ad",
      "titleBar.activeBackground": "#224484",
      "titleBar.activeForeground": "#e7e7e7",
      "titleBar.border": "#224484",
      "titleBar.inactiveBackground": "#22448499",
      "titleBar.inactiveForeground": "#e7e7e799"
   },
   "peacock.remoteColor": "#224484",
   "typescript.tsdk": "node_modules/typescript/lib",
   "prettier-eslint-typescript.eslintConfig": {
      "rules": {
         "curly": "error",
         "dot-notation": "error",
         "no-undef-init": "error",
         "no-useless-rename": "error",
         "no-useless-return": "error",
         "no-var": "error",
         "object-shorthand": "error",
         "one-var": [ "error", { "initialized": "never", "uninitialized": "always" } ],
         "prefer-const": "error",
         "prefer-template": "error",
         "array-bracket-spacing": [ "error", "always" ],
         "generator-star-spacing": [ "error", "both" ],
         "space-before-function-paren": [ "off", "always" ],
         "yield-star-spacing": [ "error", "both" ]
      }
   },
   "prettier-eslint-typescript.prettierOptions": {
      "arrowParens": "avoid",
      "bracketSpacing": true,
      "printWidth": 120,
      "quoteProps": "as-needed",
      "singleQuote": true,
      "tabWidth": 2,
      "trailingComma": "none",
      "useTabs": false
   }
}
