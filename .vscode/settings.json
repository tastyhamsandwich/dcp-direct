{
   "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#f8b3b5",
      "activityBar.background": "#f8b3b5",
      "activityBar.foreground": "#15202b",
      "activityBar.inactiveForeground": "#15202b99",
      "activityBarBadge.background": "#11920d",
      "activityBarBadge.foreground": "#e7e7e7",
      "commandCenter.border": "#15202b99",
      "editorGroup.border": "#f8b3b5",
      "panel.border": "#f8b3b5",
      "sash.hoverBorder": "#f8b3b5",
      "sideBar.border": "#f8b3b5",
      "statusBar.background": "#f48487",
      "statusBar.border": "#f48487",
      "statusBar.debuggingBackground": "#84f4f1",
      "statusBar.debuggingBorder": "#84f4f1",
      "statusBar.debuggingForeground": "#15202b",
      "statusBar.foreground": "#15202b",
      "statusBarItem.hoverBackground": "#f05559",
      "statusBarItem.remoteBackground": "#f48487",
      "statusBarItem.remoteForeground": "#15202b",
      "tab.activeBorder": "#f8b3b5",
      "titleBar.activeBackground": "#f48487",
      "titleBar.activeForeground": "#15202b",
      "titleBar.border": "#f48487",
      "titleBar.inactiveBackground": "#f4848799",
      "titleBar.inactiveForeground": "#15202b99"
   },
   "peacock.remoteColor": "#f48487",
   "typescript.tsdk": "node_modules/typescript/lib",
   "prettier-eslint-typescript.eslintConfig": {
      "rules": {
         "curly": "error",
         "dot-notation": "error",
         "no-undef-init": "error",
         "no-useless-rename": "error",
         "no-useless-return": "error",
         "no-var": "error",
         "object-shorthand": "error",
         "one-var": [ "error", { "initialized": "never", "uninitialized": "always" } ],
         "prefer-const": "error",
         "prefer-template": "error",
         "array-bracket-spacing": [ "error", "always" ],
         "generator-star-spacing": [ "error", "both" ],
         "space-before-function-paren": [ "off", "always" ],
         "yield-star-spacing": [ "error", "both" ]
      }
   },
   "prettier-eslint-typescript.prettierOptions": {
      "arrowParens": "avoid",
      "bracketSpacing": true,
      "printWidth": 120,
      "quoteProps": "as-needed",
      "singleQuote": true,
      "tabWidth": 2,
      "trailingComma": "none",
      "useTabs": false
   }
}
