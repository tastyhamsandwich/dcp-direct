{
  "name": "dcp-direct",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "devnet": "next dev --turbopack -p 3003 -H 172.21.87.173",
    "dev": "next dev --turbopack -p 3003",
    "devsock": "tsx src/server.ts",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm devsock\"",
    "dev:net": "concurrently \"pnpm devnet\" \"pnpm devsock\"",
		"dev:net:test": "concurrently \"pnpm devsock\" \"NODE_ENV=development HOSTNAME=0.0.0.0 PORT=3003 next dev\"",
		"test-server": "NODE_ENV=development DEBUG=express:* tsx src/test.ts",
    "start": "next start",
		"build": "next build",
    "lint": "next lint",
    "dev:sculpt": "concurrently \"pnpm dev\" \"pnpm devsock\" \"sculpt\"",
    "dev:sculpt:net": "concurrently \"pnpm devnet\" \"pnpm devsock\" \"sculpt\""
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.8",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cookie": "^1.0.2",
    "express": "^4.21.2",
    "framer-motion": "^12.6.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.479.0",
    "next": "15.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-image-crop": "^11.0.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.0.2",
    "tsx": "^4.19.3",
    "uuid": "^11.1.0",
    "websockets": "^0.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@tailwindcss/postcss": "^4.0.14",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^20.17.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.2.1",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "core-js-pure",
      "esbuild",
      "supabase"
    ]
  },
  "packageManager": "pnpm@10.10.0"
}